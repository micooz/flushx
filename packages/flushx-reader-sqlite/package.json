{
  "name": "flushx-reader-sqlite",
  "version": "1.0.0",
  "description": "sqlite3 reader plugin for flushx",
  "main": "lib/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "compile": "rm -rf lib && tsc",
